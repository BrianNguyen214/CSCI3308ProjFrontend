       imgInstructions: [],\n            vidInstructions: [],\n            articleHeaders: [],\n            articleStringers: [],\n            placedHeaders: [],\n\n            articleTitle: null,\n            articleDescription: null,\n            articlePic: null,\n            articleDatePub: null,\n            articleAuthor: null,\n            articleURL: null,\n            articleIsPDF: null,\n            articleToken1: null,\n            articleToken2: null,\n            articleToken3: null,\n\n            //url: \"http://127.0.0.1:8000/\",\n            url: \"https://websiteapi.udana.systems/\",\n        }\n    },\n\n    methods: {\n        articleParser: function() {\n            \n            var str = this.entireArticle;\n            var begin = 0;\n            var end = 0;\n            var stringer = \"\";\n            var foundImgStart = false;\n            var foundImgEnd = false;\n            var foundVidStart = false;\n            var foundVidEnd = false;\n            var foundHeaderStart = false;\n            var HSIndex = 0;\n            var foundHeaderEnd = false;\n            var HEIndex = 0;\n            var newHeader = \"\";\n            var needHMPair = false;\n            var numStringersAdded = 0;\n            var numParaAdded = 0;\n            var numHeadersAdded = 0;\n            var indexFoundVidStart = 0;\n\n            for (var i = 0; i < this.entireArticle.length; i++){\n                if (str.substring(i,i+3) == \"<*<\") {\n                    foundImgStart = true;\n                }\n                if (str.substring(i,i+3) == \">*>\") {\n                    foundImgEnd = true;\n                }\n                if (foundImgStart == true && foundImgEnd == false && str[i] != ',' && i > 2) {\n                    this.imgInstructions.push(str[i]);\n                }\n\n                if (str.substring(i,i+3) == \"<&<\") {\n                    foundVidStart = true;\n                    indexFoundVidStart = i\n                }\n                if (str.substring(i,i+3) == \">&>\") {\n                    foundVidEnd = true;\n                }\n                if (foundVidStart == true && foundVidEnd == false && str[i] != ',' && i > (indexFoundVidStart + 2)) {\n                    this.vidInstructions.push(str[i]);\n                }\n\n                if (str.substring(i,i+3) == \"<||\") {\n                    //console.log(\"Found HeaderStart\")\n                    foundHeaderStart = true;\n                    needHMPair = true;\n                    HSIndex = i+3;\n                }\n                if (str.substring(i,i+3) == \"||>\") {\n                    //console.log(\"Found HeaderEnd\")\n                    foundHeaderEnd = true;\n                    HEIndex = i;\n                }\n                if (foundHeaderStart == true && foundHeaderEnd == true && needHMPair == true) {\n                    newHeader = str.substring(HSIndex, HEIndex);\n                    //console.log(newHeader);\n                    this.articleHeaders.push(newHeader);\n                    foundHeaderStart = false;\n                    foundHeaderEnd = false;\n                    needHMPair = false;\n                }\n\n                if (str.substring(i,i+2) == \"**\") {\n                    end = i;\n                    stringer = str.substring(begin, end);\n                    begin = i+2;\n\n                    this.articleStringers.push(stringer)\n                    \n                    if (stringer[0] != '<') {\n                        this.articleParagraphs.push(stringer);\n                    }\n                    \n                    if (stringer[0] != \"<\" && numStringersAdded > 0) {\n                        //console.log(\"Added a para\")\n                        numParaAdded += 1;\n                        \n                        if (this.articleStringers[numStringersAdded-1][1] == '|'){\n                            //console.log(\"Para has a header\")\n                            this.placedHeaders.push(this.articleHeaders[numHeadersAdded])\n                            numHeadersAdded += 1\n                        } else {\n                            //console.log(\"Para doesn't have a header\")\n                            this.placedHeaders.push(\"@@\")\n                        }\n                        \n                    } else if (numStringersAdded == 0) {\n                        //console.log(\"Added instructions\");\n                    } else if (stringer[0] == \"<\" && numStringersAdded > 0){\n                        //console.log(\"Added header\")\n                    }\n                    numStringersAdded += 1;\n\n                }\n            }\n            console.log(this.articleStringers)\n            console.log(this.articleParagraphs)\n            console.log(this.articleHeaders)\n            console.log(this.imgInstructions)\n            console.log(this.vidInstructions)\n            console.log(this.placedHeaders)\n        },\n\n        imgPlacer: function() {\n            var counter = 0\n            for (var i = 0; i < this.imgInstructions.length; i++){\n                if (this.imgInstructions[i] != '_') {\n                    //console.log(this.images[counter])\n                    this.placedImages.push(this.images[counter])\n                    counter = counter + 1\n                }\n                else {\n                    this.placedImages.push('_')\n                }\n            }\n            console.log(this.placedImages)\n        },\n\n        vidPlacer: function() {\n            var counter = 0\n            for (var i = 0; i < this.vidInstructions.length; i++){\n                if (this.vidInstructions[i] != '_') {\n                    //console.log(this.images[counter])\n                    this.placedVideos.push(this.videos[counter])\n                    counter = counter + 1\n                }\n                else {\n                    this.placedVideos.push('_')\n                }\n            }\n        